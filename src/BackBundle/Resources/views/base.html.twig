                    </li>
                    <li>
                        <a href="#">
                            <span>RNA</span>
                        </a>
                    </li>
                </ul>
            </div>
        </aside>
        
        {% block main %}
            <div class="form-group">
                <label class="col-sm-2 col-sm-2 control-label">Default</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control">
                </div>
            </div>
        {% endblock %}                
    </section>

    <script src="bundles/back/js/jquery.dcjqaccordion.2.7.js"></script>
    <script src="bundles/back/js/jquery.scrollTo.min.js"></script> 
    <script src="bundles/back/js/common-scripts.js"></script>
